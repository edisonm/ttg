  object QuClashResource: TZQuery[7]
    Connection = SourceDataModule.DbZConnection
    AutoCalcFields = False
    ReadOnly = True
    SQL.Strings = (
      'SELECT DISTINCT'
      '  TimetableDetail.IdTimetable,'
      '  TimetableResource.IdResource,'
      '  TimetableDetail.IdDay,'
      '  TimetableDetail.IdHour,'
      '  Resource.IdResourceType,'
      '  ResourceType.NaResourceType,'
      '  Resource.NaResource,'
      '  Day.NaDay,'
      '  Hour.NaHour,'
      '  SUM(TimetableResource.NumResource) AS Occupied,'
      '  SUM(TimetableResource.NumResource) - Resource.NumResource AS Clashes'
      'FROM TimetableDetail'
      '  INNER JOIN TimetableResource'
      '   ON TimetableDetail.IdTimetable=TimetableResource.IdTimetable'
      '  AND TimetableDetail.IdActivity=TimetableResource.IdActivity'
      '  INNER JOIN Resource ON TimetableResource.IdResource=Resource.IdResource'
      '  INNER JOIN ResourceType ON Resource.IdResourceType=ResourceType.IdResourceType'
      '  INNER JOIN Day ON TimetableDetail.IdDay=Day.IdDay'
      '  INNER JOIN Hour ON TimetableDetail.IdHour=Hour.IdHour'
      'WHERE TimetableDetail.IdTimetable=:IdTimetable'
      'GROUP BY'
      '  TimetableDetail.IdTimetable,'
      '  TimetableResource.IdResource,'
      '  TimetableDetail.IdDay,'
      '  TimetableDetail.IdHour,'
      '  Resource.IdResourceType,'
      '  ResourceType.NaResourceType,'
      '  Resource.NaResource'
      'HAVING Clashes > 0'
      'ORDER BY TimetableDetail.IdTimetable, ResourceType.NaResourceType, Resource.NaResource'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'IdTimetable'
        ParamType = ptUnknown
      end>
    DataSource = DataSource
    MasterFields = 'IdTimetable'
    LinkedFields = 'IdTimetable'
