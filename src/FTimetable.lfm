  object QuBrokenSessionHour: TZQuery[12]
    Connection = SourceDataModule.DbZConnection
    SortedFields = 'IdTimetable'
    AutoCalcFields = False
    ReadOnly = True
    SQL.Strings = (
      'SELECT IdTimetable, IdDay, IdHour, NaDay, NaHour'
      'FROM Timetable, Day, Hour'
      'WHERE'
      '  IdTimetable=:IdTimetable'
      '  AND ('
      '    SELECT COUNT(*) FROM Period'
      '    WHERE Period.IdDay=Day.IdDay'
      '      AND Period.IdHour=Hour.IdHour'
      '  ) IS NULL'
      '  AND ('
      '  SELECT'
      '    MAX(TimetableDetail.IdHour)'
      '  FROM'
      '    TimetableDetail INNER JOIN TimetableDetail TimetableDetail1'
      '       ON TimetableDetail.IdTimetable=TimetableDetail1.IdTimetable'
      '      AND TimetableDetail.IdActivity=TimetableDetail1.IdActivity'
      '      AND TimetableDetail.Session=TimetableDetail1.Session'
      '      AND TimetableDetail.IdDay=TimetableDetail1.IdDay'
      '    WHERE TimetableDetail.IdTimetable=Timetable.IdTimetable'
      '      AND TimetableDetail.IdDay=Day.IdDay'
      '      AND TimetableDetail.IdHour>Hour.IdHour'
      '      AND TimetableDetail1.IdHour<Hour.IdHour'
      '  ) IS NOT NULL'
    )
    Params = <    
      item
        DataType = ftUnknown
        Name = 'IdTimetable'
        ParamType = ptUnknown
      end>
