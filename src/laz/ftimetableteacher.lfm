  object QuTimetableTeacher: TZQuery[7]
    Connection = SourceDataModule.DbZConnection
    SortedFields = 'IdTimetable;IdTeacher'
    AutoCalcFields = False
    OnCalcFields = QuTimetableTeacherCalcFields
    SQL.Strings = (
      'select'
      ' IdTimetable,'
      ' IdTeacher,'
      ' Distribution.IdCategory,'
      ' Distribution.IdParallel,'
      ' IdHour,'
      ' IdDay,'
      ' Distribution.IdTheme,'
      ' 0 as IsAssistance'
      'from'
      ' TimetableDetail inner join Distribution on'
      ' (TimetableDetail.IdTheme = Distribution.IdTheme)'
      ' and (TimetableDetail.IdCategory = Distribution.IdCategory)'
      ' and (TimetableDetail.IdParallel = Distribution.IdParallel)'
      'where IdTimetable=:IdTimetable'
      ' and IdTeacher=:IdTeacher'
      'union all'
      'select'
      ' IdTimetable,'
      ' Assistance.IdTeacher as IdTeacher,'
      ' Distribution.IdCategory,'
      ' Distribution.IdParallel,'
      ' IdHour,'
      ' IdDay,'
      ' Distribution.IdTheme,'
      ' 1 as IsAssistance'
      'from'
      ' Assistance inner join Distribution on'
      '  Assistance.IdTheme = Distribution.IdTheme'
      '  and Assistance.IdCategory = Distribution.IdCategory'
      '  and Assistance.IdParallel = Distribution.IdParallel'
      ' inner join TimetableDetail on'
      '  TimetableDetail.IdTheme = Distribution.IdTheme'
      '  and TimetableDetail.IdCategory = Distribution.IdCategory'
      '  and TimetableDetail.IdParallel = Distribution.IdParallel'
      'where IdTimetable=:IdTimetable'
      ' and Assistance.IdTeacher=:IdTeacher'
    )
