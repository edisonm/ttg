  object QuTimetableClass: TZQuery[7]
    Connection = SourceDataModule.DbZConnection
    SortedFields = 'IdTimetable;IdLevel;IdSpecialization;IdGroupId'
    AutoCalcFields = False
    OnCalcFields = QuTimetableClassCalcFields
    ReadOnly = True
    SQL.Strings = (
      'select'
      '  TimetableDetail.IdTimetable,'
      '  TimetableDetail.IdLevel,'
      '  TimetableDetail.IdSpecialization,'
      '  TimetableDetail.IdGroupId,'
      '  IdHour,'
      '  IdDay,'
      '  TimetableDetail.IdSubject,'
      '  Distribution.IdTeacher,'
      '  NaSubject,'
      '  LnTeacher,'
      '  NaTeacher'
      'from'
      '  ((TimetableDetail inner join Distribution on'
      '  (TimetableDetail.IdSubject = Distribution.IdSubject)'
      '  and (TimetableDetail.IdLevel = Distribution.IdLevel)'
      '  and (TimetableDetail.IdSpecialization = Distribution.IdSpecialization)'
      '  and (TimetableDetail.IdGroupId = Distribution.IdGroupId))'
      '  inner join Subject on (TimetableDetail.IdSubject = Subject.IdSubject))'
      '  inner join Teacher on (Distribution.IdTeacher = Teacher.IdTeacher)'
      'where'
      '  IdTimetable=:IdTimetable'
      'and TimetableDetail.IdLevel=:IdLevel'
      'and TimetableDetail.IdSpecialization=:IdSpecialization'
      'and TimetableDetail.IdGroupId=:IdGroupId'
