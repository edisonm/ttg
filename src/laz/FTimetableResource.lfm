  object QuTimetableResource: TZQuery[7]
    Connection = SourceDataModule.DbZConnection
    AutoCalcFields = False
    OnCalcFields = QuTimetableResourceCalcFields
    SQL.Strings = (
      'SELECT'
      ' TimetableDetail.IdTimetable,'
      ' TimetableResource.IdResource as IdResource,'
      ' Activity.IdActivity,'
      ' IdHour,'
      ' IdDay,'
      ' Activity.IdTheme'
      'FROM'
      'TimetableDetail INNER JOIN TimetableResource'
      '   ON TimetableDetail.IdTimetable=TimetableResource.IdTimetable'
      '  AND TimetableDetail.IdActivity=TimetableResource.IdActivity'
      'INNER JOIN Activity'
      '  ON Activity.IdActivity=TimetableResource.IdActivity'
      'INNER JOIN Theme ON Activity.IdTheme=Theme.IdTheme'
      'WHERE TimetableDetail.IdTimetable=:IdTimetable'
      '  AND TimetableResource.IdResource=:IdResource'
